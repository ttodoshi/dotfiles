[user]
	name = todoshi
	email = ttodoshi@users.noreply.github.com
[alias]
	cmts = log --graph --abbrev-commit --decorate --all --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(dim white) - %an%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n %C(white)%s%C(reset)'
	chk = checkout
	ct = commit
[credential]
	helper = store
[core]
	editor = nvim
